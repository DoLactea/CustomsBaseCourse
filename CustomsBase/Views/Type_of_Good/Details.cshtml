@model CustomsBase.Models.Type_of_Good
@{ 
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h3>
    <a asp-area="" asp-controller="Type_of_Good" asp-action="Index">Back</a>
</h3>

<h2>Goods № @Model.Types_of_goodsID</h2>
<table>
    <tr>
        <td>
            @Html.LabelFor(model=>model.Types_of_goodsID, "Goods ID")
        <td>
            @Html.DisplayFor(model => model.Types_of_goodsID)
        </td>
    </tr>
    <tr>
        <td>
            @Html.LabelFor(model => model.Name, "Goods Name")
        </td>
        <td>
            @Html.DisplayFor(model => model.Name)
        </td>
    </tr>
    <tr>
        <td>
            @Html.LabelFor(model => model.Unit, "Unit")
        </td>
        <td>
            @Html.DisplayFor(model => model.Unit)
        </td>
    </tr>
    <tr>
        <td>
            @Html.LabelFor(model => model.Amount_of_duty, "Amount of duty")
        </td>
        <td>
            @Html.DisplayFor(model => model.Amount_of_duty)
        </td>
    </tr>
</table>


